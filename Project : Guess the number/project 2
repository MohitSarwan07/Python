import random
i=1
n = random.randint(1,100)
num = -1
while (num != n):
    num = int(input("Guess the number: "))
    if (n<num):
        print("Lower number please")
        i += 1
    elif (n>num):
        print("Higher number please")
        i += 1


print(f"You Guessed it right! Number of guesses used: {i}")
